{
  "name": "babel-plugin-transform-koa2-async-to-generator",
  "version": "0.0.0-development",
  "description": "Turn Koav2 async Syntax to Koav1 generators",
  "repository": "https://github.com/gerard2p/babel-plugin-transform-koa2-async-to-generator",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "commit": "git add -A & git-cz",
    "convert": "babel src/index.js --out-file lib/index.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "babel-plugin"
  ],
  "dependencies": {
    "babel-helper-remap-koa2-async-to-generator": "^1.0.0",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-runtime": "^6.0.0"
  },
  "devDependencies": {
    "babel-helper-plugin-test-runner": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "files": [
    "README.md",
    "lib/index.js"
  ]
}
